{
  "apps_link" : {
    "path": "<%= config[:link_prefix] %>/apps",
    "text": "Web Suite"
  },
  "config_packs_link" : {
    "path": "<%= config[:link_prefix] %>/config-packs",
    "text": "Config Packs"
  },
  "apps" : 
    [<% app_items.each_with_index do |item, idx| %>
    <%= idx == 0 ? ' ' : ',' %> <%= item.reps.fetch(:data).compiled_content %>
    <% end %>],
  "config_packs" :
    [<% config_pack_items.each_with_index do |item, idx| %>
    <%= idx == 0 ? ' ' : ',' %> <%= item.reps.fetch(:data).compiled_content %>
    <% end %>],
  "sidebar" : <%= (sidebar.compiled_content || '').inspect %>
}
